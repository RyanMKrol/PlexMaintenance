{
  "name": "plex-maintenance",
  "version": "2.0.0",
  "description": "",
  "type": "module",
  "//" : "experimental-specifier-resolution=node allows me to not have to specify .../index.js in my imports",
  "scripts": {
    "lint": "eslint src",
    "script:auditAll": "npm run script:auditDuplicateFiles && npm run script:auditFileBitrates && npm run script:auditNewSeasons",
    "script:auditDuplicateFiles": "npm run lint && node --experimental-specifier-resolution=node src/scripts/auditDuplicateFiles",
    "script:auditFileBitrates": "npm run lint && node --experimental-specifier-resolution=node src/scripts/auditFileBitrates",
    "script:auditNewSeasons": "npm run lint && node --experimental-specifier-resolution=node src/scripts/auditNewSeasons"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/RyanMKrol/PlexMaintenance.git"
  },
  "author": "Ryan Krol",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/RyanMKrol/PlexMaintenance/issues"
  },
  "homepage": "https://github.com/RyanMKrol/PlexMaintenance#readme",
  "devDependencies": {
    "eslint": "^8.46.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.28.0",
    "eslint-plugin-jsdoc": "^46.4.6"
  },
  "dependencies": {
    "dotenv": "^16.3.1",
    "fast-xml-parser": "^4.2.7",
    "node-fetch": "^3.3.2"
  }
}
